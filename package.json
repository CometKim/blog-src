    "develop": "run-p develop:*",
    "develop:gatsby": "gatsby develop",
    "develop:codegen": "graphql-codegen -o -w",
    "serve": "gatsby serve",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:ci": "yarn test --ci --coverage --reporters=default --reporters=jest-junit"
