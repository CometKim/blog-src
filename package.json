{
    "name": "cometkim-blog-src",
    "description": "Source of Hyeseong's Blog",
    "version": "0.0.1",
    "author": "Hyeseong Kim <cometkim.kr@gmail.com>",
    "keywords": [
        "gatsby",
        "typescript",
        "blog"
    ],
    "license": "MIT",
    "main": "n/a",
    "private": true,
    "scripts": {
        "update-posts": "git submodule update --init --remote",
        "build": "yarn update-posts && gatsby build",
        "develop": "gatsby develop",
        "format": "tslint --fix -c tslint.json src/**/*.tsx?",
        "test": "jest"
    },
    "jest": {
        "verbose": true,
        "testRegex": "/__tests__/.*\\.test\\.(js|ts)x?$",
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/.cache/"
        ],
        "modulePaths": [
            "<rootDir>/src/"
        ],
        "moduleNameMapper": {
            "node-utils": "<rootDir>/node-utils.js"
        },
        "moduleFileExtensions": [
            "js",
            "jsx",
            "ts",
            "tsx",
            "json"
        ]
    },
    "dependencies": {
        "gatsby": "^2.0.9",
        "gatsby-plugin-canonical-urls": "^2.0.5",
        "gatsby-plugin-feed": "^2.0.5",
        "gatsby-plugin-google-analytics": "^2.0.6",
        "gatsby-plugin-manifest": "^2.0.4",
        "gatsby-plugin-netlify": "^2.0.0",
        "gatsby-plugin-offline": "^2.0.5",
        "gatsby-plugin-react-helmet": "^3.0.0",
        "gatsby-plugin-resolve-src": "^2.0.0-beta.1",
        "gatsby-plugin-sharp": "^2.0.5",
        "gatsby-plugin-sitemap": "^2.0.1",
        "gatsby-plugin-styled-components": "^3.0.0",
        "gatsby-plugin-twitter": "^2.0.5",
        "gatsby-plugin-typescript": "^2.0.0",
        "gatsby-remark-autolink-headers": "^2.0.6",
        "gatsby-remark-copy-linked-files": "^2.0.5",
        "gatsby-remark-embed-youtube": "^0.0.7",
        "gatsby-remark-emojis": "^0.2.3",
        "gatsby-remark-external-links": "^0.0.4",
        "gatsby-remark-images": "^2.0.3",
        "gatsby-remark-katex": "^2.0.5",
        "gatsby-remark-prismjs": "^3.0.1",
        "gatsby-remark-responsive-iframe": "^2.0.5",
        "gatsby-source-filesystem": "^2.0.1",
        "gatsby-transformer-remark": "^2.1.4",
        "prismjs": "^1.15.0",
        "react": "^16.5.2",
        "react-dom": "^16.5.2",
        "react-helmet": "^5.2.0",
        "react-icons-kit": "^1.1.12",
        "remark-parse": "^5.0.0",
        "styled-components": "^3.4.9"
    },
    "devDependencies": {
        "@babel/core": "^7.1.0",
        "@types/jest": "^23.3.2",
        "@types/node": "^10.11.0",
        "@types/react": "^16.4.14",
        "@types/react-dom": "^16.0.7",
        "@types/react-helmet": "^5.0.7",
        "babel-core": "^7.0.0-bridge.0",
        "babel-plugin-styled-components": "^1.7.1",
        "jest": "^23.6.0",
        "tslint": "^5.11.0"
    }
}
